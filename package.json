{
  "name": "@jonales/n8n-nodes-oracle-database-thin",
  "version": "1.2.0",
  "description": "Oracle Database node with embedded thin client support - no external Oracle CLI required",
  "keywords": [
    "n8n-community-node-package",
    "oracle",
    "database",
    "thin-client",
    "embedded"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jonales/n8n-nodes-oracle-database-thin#readme",
  "author": {
    "name": "JÃ´natas Meireles",
    "email": "jonatas.mei@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonales/n8n-nodes-oracle-database-thin.git"
  },
  "main": "dist/nodes/Oracle/OracleDatabase.node.js",
  "types": "dist/nodes/Oracle/OracleDatabase.node.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Oracle.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Oracle/OracleDatabase.node.js"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/oracledb": "^5.3.0",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.5.4",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.2",
    "typescript": "~5.4.0",
    "@types/node": "^20.11.0"
  },
  "dependencies": {
    "oracledb": "^6.3.0"
  }
}